{
  "version": 2,
  "builds": [
    {
      "src": "get_weather.py",
      "use": "@vercel/python"
    },
    {
      "src": "aura/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "get_weather.py"
    },
    {
      "src": "/briefing",
      "dest": "get_weather.py"
    },
    {
      "src": "/(.*)",
      "dest": "aura/$1"
    }
  ]
}